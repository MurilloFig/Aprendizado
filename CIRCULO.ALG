Algoritmo "circulo"

Var
     raio : real
     raioDois : real
     area : real
     p : real

Inicio

       escreval("Digite o valor do raio do circulo:")
       leia(raio)

       p <- Pi:8:2
       raioDois <- Exp(raio, 2)
       
       area <- p * raioDois
       
       escreval("AREA =", area)







Fimalgoritmo